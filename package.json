{
  "name": "bloggo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run tw:watch\" \"next dev  -p 5000\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tw:watch": "npx @tailwindcss/cli -i ./src/app/globals.css -o ./.next/static/css/app/layout.css --watch",
    "db:push": "prisma generate && prisma db push",
    "db:reset": "prisma generate && prisma db push --force-reset && prisma db seed "
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdxeditor/editor": "^3.26.0",
    "@next/mdx": "^15.2.2",
    "@prisma/client": "^6.5.0",
    "@sinclair/typebox": "^0.34.33",
    "@vercel/blob": "^0.27.3",
    "bcrypt": "^6.0.0",
    "next": "^15.2.5",
    "next-auth": "^4.24.11",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-day-picker": "^9.6.6",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "remark-gfm": "^4.0.1",
    "sass": "^1.86.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/cli": "^4.0.13",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-day-picker": "^5.2.1",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
